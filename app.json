{
  "expo": {
    "name": "scorte_checker",
    "slug": "scorte_checker",
    "scheme": "scorte-checker",
    "icon": "./assets/icon.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "extraPods": [
              {
                "name": "simdjson",
                "configurations": [
                  "Debug",
                  "Release"
                ],
                "path": "../node_modules/@nozbe/simdjson",
                "modular_headers": true
              }
            ]
          }
        }
      ],
      "expo-sqlite",
      "expo-router",
      "expo-camera",
      "@react-native-community/datetimepicker"
    ],
    "android": {
      "icon": "./assets/icon.png",
      "package": "com.anonymous.scorte_checker",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2980b9"
      }
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.scorte-checker"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1c6297bc-18b5-4518-877c-70f50dff2b97"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1c6297bc-18b5-4518-877c-70f50dff2b97"
    }
  }
}
