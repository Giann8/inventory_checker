{
  "expo": {
    "name": "scorte_checker",
    "slug": "scorte_checker",
    "scheme": "scorte-checker",
    "version": "1.0.0",
    "orientation": "portrait",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "extraPods": [
              {
                "name": "simdjson",
                "configurations": [
                  "Debug",
                  "Release"
                ],
                "path": "../node_modules/@nozbe/simdjson",
                "modular_headers": true
              }
            ]
          }
        }
      ],
      "expo-sqlite",
      "expo-router",
      "expo-camera",
      "@react-native-community/datetimepicker"
    ],
    "android": {
      "package": "com.anonymous.scorte_checker"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.scorte-checker"
    }
  }
}
