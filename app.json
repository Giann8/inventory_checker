{
  "expo": {
    "name": "scorte_checker",
    "slug": "scorte_checker",
    "version": "1.0.0",
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "extraPods": [
              {
                "name": "simdjson",
                "configurations": [
                  "Debug",
                  "Release"
                ],
                "path": "../node_modules/@nozbe/simdjson",
                "modular_headers": true
              }
            ]
          }
        }
      ],
      "expo-sqlite",
      "@morrowdigital/watermelondb-expo-plugin"
    ]
  }
}